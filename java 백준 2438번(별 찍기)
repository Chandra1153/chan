import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int count = scn.nextInt();

        for(int i=1; i <= count; i++) {
            for(int j=0; j < i; j++) {
                System.out.print("*");
            } 
            System.out.println();
        }
    }
}

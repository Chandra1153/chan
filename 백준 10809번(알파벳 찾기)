#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main() {
	int i, j, k = 0;
	char c;
	int pos[26] = { 0, };
	char arr[100] = { 0, };
	gets(arr);
	for (i = 97; i < 123; i++) {
		for (j = 0; j < strlen(arr); j++) {
			c = arr[j];
			if (i == (c - 48)) {
				pos[k++] = j;
				continue;
			}
			else
				pos[k++] = -1;
		}
	}
	for (i = 0; i < 26; i++) 
		printf("%d ", pos[i]);
	return 0;
}

n, m = map(int, input().split())
card = list(map(int, input().split()))
arr = []
for i in range(n-2):
    sum = 0
    sum += card[i]
    for j in range(i+1, n-1):
        if sum + card[j] >= m:
            continue
        sum += card[j]
        for k in range(j+1, n):
            if sum + card[k] > m:
                continue
            sum += card[k]
    arr.append(sum)
print(max(arr))

#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

// num_list_len은 배열 num_list의 길이입니다.
int* solution(int num_list[], size_t num_list_len) {
    // return 값은 malloc 등 동적 할당을 사용해주세요. 할당 길이는 상황에 맞게 변경해주세요.
    int* answer = (int*)malloc(5*sizeof(int));
    int max, tmp, pos;
    for(int i = 0; i < num_list_len - 1; i++) {
        max = 0;
        for(int j = 0; j < num_list_len - i; j++) {
            if(max < num_list[j]) {
                max = num_list[j];
                pos = j;
            }
        }
        tmp = num_list[num_list_len - 1 - i];
        num_list[num_list_len - 1 - i] = num_list[pos];
        num_list[pos] = tmp;
    }
    for(int i = 0; i < 5; i++) {
        answer[i] = num_list[i];
    }
    return answer;
}

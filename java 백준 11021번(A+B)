import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int count = scn.nextInt();

        for(int i=1; i <= count; i++) {
            int a = scn.nextInt();
            int b = scn.nextInt();
            System.out.println("Case #" + i + ": " + (a+b));
        }
    }
}

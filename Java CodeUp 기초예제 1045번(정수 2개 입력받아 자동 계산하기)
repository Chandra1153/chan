import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int a, b;
        Scanner scn = new Scanner(System.in);
        a = scn.nextInt();
        b = scn.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println((int) (a/b));
        System.out.println(a%b);
        System.out.printf("%.2f\n", (float)a/(float)b);
    }
}

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int n,a,b;
        Scanner scn = new Scanner(System.in);
        n = scn.nextInt();

        for(int i=0; i < n; i++) {
            a = scn.nextInt();
            b = scn.nextInt();
            System.out.println(a+b);
        }
    }
}

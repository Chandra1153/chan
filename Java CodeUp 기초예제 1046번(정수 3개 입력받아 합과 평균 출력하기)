import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int a, b, c;
        Scanner scn = new Scanner(System.in);
        a = scn.nextInt();
        b = scn.nextInt();
        c = scn.nextInt();

        System.out.println(a+b+c);
        System.out.printf("%.1f\n", (float) (a+b+c)/3);
    }
}

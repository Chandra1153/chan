import sys
import math
input = sys.stdin.readline
t = int(input())
for _ in range(t):
    n = int(input())
    prime = False
    while prime == False:
        prime = True
        for i in range(2, int(math.sqrt(n)) + 1):
            if n % i == 0:
                prime = False
                break
        if prime == True:
            print(n)
        else:
            n += 1

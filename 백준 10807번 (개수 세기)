#include <stdio.h>

int main() {
	int amount, snum, count = 0, num;
	int arr[100] = { 0, };
	scanf("%d", &amount);
	for (int i = 0; i < amount; i++) {
		scanf("%d", &num);
		arr[i] = num;
	}
	scanf("%d", &snum);
	for (int i = 0; i < amount; i++) {
		if (arr[i] == snum)
			count++;
	}
	printf("%d", count);
}

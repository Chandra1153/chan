result = []
t = int(input())
for i in range(t):
    s = input()
    if len(s) == 1:
        result.append(s[0])
        result.append(s[0])
    if len(s) > 1:
        result.append(s[0])
        result.append(s[-1])
for i in range(1, len(result), 2):
    if i % 2 == 1:
        print(result[i-1], result[i], sep="",end="")
        print(" ")

import sys
import math
input = sys.stdin.readline

def prime(n):
    for i in range(2, int(math.sqrt(n))+1):
        if n % i == 0:
            return False
    return True

while True:
    n = int(input())
    if n == 0:
        break
    cnt = 0
    for i in range(n+1, n*2+1):
        if prime(i):
            cnt += 1
    print(cnt)
